		workflow
		{
		form
		{
			Direct_to_DFG as "Direct to DFG"
			{
				type =  form
				form = Bare_Calculator

				record event = on add or edit

				on success
				{
					actions  (Aluminium_or_Copper == "Aluminium" && Strip_or_Wire == "Strip" && Insulation == "Dfg 225 yarn")
					{
						openURL("#Form:Dfg_225_yarn_alum","new window");
					}
					actions  (Aluminium_or_Copper == "Aluminium" && Strip_or_Wire == "Wire" && Insulation == "Dfg 225 yarn")
					{
						openURL("#Form:Dfg_Al_Wire_225","new window");
					}
					actions  (Aluminium_or_Copper == "Copper" && Strip_or_Wire == "Strip" && Insulation == "Dfg 225 yarn")
					{
						openURL("#Form:Dfg_Cu_Strip_225_yarn","new window");
					}
					actions  (Aluminium_or_Copper == "Copper" && Strip_or_Wire == "Wire" && Insulation == "Dfg 225 yarn")
					{
						openURL("#Form:Dfg_Cu_Wire_225","new window");
					}
				}

			}
			Direct_to_Polyester as "Direct to Polyester"
			{
				type =  form
				form = Bare_Calculator

				record event = on add or edit

				on success
				{
					actions  (Aluminium_or_Copper == "Aluminium" && Strip_or_Wire == "Strip" && Insulation == "Polyester")
					{
						openURL("#Form:Poly_Alu_Str","new window");
					}
					actions  (Aluminium_or_Copper == "Aluminium" && Strip_or_Wire == "Wire" && Insulation == "Polyester")
					{
						openURL("#Form:Copy_of_Dfg_Al_Wire_225","new window");
					}
					actions  (Aluminium_or_Copper == "Copper" && Strip_or_Wire == "Strip" && Insulation == "Polyester")
					{
						openURL("#Form:Poly_Cu_Str","new window");
					}
					actions  (Aluminium_or_Copper == "Copper" && Strip_or_Wire == "Wire" && Insulation == "Polyester")
					{
						openURL("#Form:Poly_Cu_Wire","new window");
					}
				}

			}
			Direct_to_Poly_Fibre as "Direct to Poly + Fibre"
			{
				type =  form
				form = Bare_Calculator

				record event = on add or edit

				on success
				{
					actions  (Aluminium_or_Copper == "Aluminium" && Strip_or_Wire == "Strip" && Insulation == "Poly + Dfg 225")
					{
						openURL("#Form:Poly_Dfg_Al_Str_225","new window");
					}
					actions  (Aluminium_or_Copper == "Copper" && Strip_or_Wire == "Strip" && Insulation == "Poly + Dfg 225")
					{
						openURL("#Form:Poly_Dfg_Cu_Str_225","new window");
					}
					actions  (Aluminium_or_Copper == "Aluminium" && Strip_or_Wire == "Wire" && Insulation == "Poly + Dfg 225")
					{
						openURL("#Form:Poly_Dfg_Alu_Wire_225","new window");
					}
					actions  (Aluminium_or_Copper == "Copper" && Strip_or_Wire == "Wire" && Insulation == "Poly + Dfg 225")
					{
						openURL("#Form:Poly_Dfg_Cu_Wire_225","new window");
					}
				}

			}
			Direct_to_DCC_32s_ as "Direct to DCC (32s)"
			{
				type =  form
				form = Bare_Calculator

				record event = on add or edit

				on success
				{
					actions  (Aluminium_or_Copper == "Aluminium" && Strip_or_Wire == "Strip" && Insulation == "Cotton 32s ( alu )")
					{
						openURL("#Form:DCC_Alu_Str_32_s","new window");
					}
				}

			}
			Direct_to_Form_Properties as "Direct to - Form Properties"
			{
				type =  presuccess
				form = Poly_Alu_Str
				on start
				{
					actions 
					{
						openURL("#Form:Poly_Alu_Str","new window");
					}
				}
			}
			on_polyaluform_submit as "on polyaluform submit"
			{
				type =  form
				form = Front

				record event = on add
					status = inactive

				on success
				{
					actions 
					{
						sendmail
						(
							from : "zoho.adminuserid"
							to : "harsh007desai@gmail.com"
							subject : "Record Added"
							message : "<div>${zoho.loginuserid} has added a record in form Frontpolytest<br></div>"
							attachments : template : _default1 as pdf
						)
					}
				}

			}
		}






